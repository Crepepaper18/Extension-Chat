{
    "name": "Chat",
    "version": "0.1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {}
,
    "host_permissions": ["http://localhost:9000/*"],
    "web_accessible_resources": [
        {
            "resources": ["socket.io.min.js"],
            "matches": ["<all_urls>"]
        }
    ],
    "permissions": ["activeTab"]
}
